{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.rl",
  "version": "1.0",
  "author": "renlei",
  "createTime": "2024-11-17",
  "fileConfig": {
    "inputRootPath": "F:/code/backend/rl-generater",
    "outputRootPath": "output",
    "resourceRootPath": "F:/code/backend/rl-generater/acm-template",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/rl/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/rl/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "name": "loop",
         "type":"boolean",
        "description":"是否循环",
        "defaultValue":false,
        "abbr": "l"
      },
      {
        "name": "author",
        "type":"String",
      "description":"作者",
      "defaultValue":"renlei",
      "abbr": "a"
      },
      {
        "name": "output",
        "type":"String",
      "description":"输出路径",
      "defaultValue":"sum =",
      "abbr": "o"
      }
    ]
  }
}
